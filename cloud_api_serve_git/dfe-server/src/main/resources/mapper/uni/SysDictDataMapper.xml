<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.df.server.mapper.sys.SysDictDataMapper">
    <select id="selectByDictTypeAndSort" resultType="com.df.server.entity.sys.SysDictDataEntity">
        select *
        from sys_dict_data
        where dict_type = #{dictType,jdbcType=VARCHAR}
        and dict_sort =#{dictSort}
    </select>
</mapper>